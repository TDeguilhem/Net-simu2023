###############################################
#### SIMULATED NETWORKS: density variation ####
###############################################


library(tidyverse)
library(igraph)
library(intergraph)
library(statnet)
library(tidygraph)
library(ggraph)

##------------------------------------------------------##
## 176 random graphs: n -> [30,50] ; density -> [0.1,0.7]
## mutual : 0.5 [half chance of reciprocity (log-odds = 0]
##------------------------------------------------------##


#1

# Sortie en pdf

pdf("XXX.pdf",   
    width = 8.27, # en inches
    height = 11.69) # en inches



par(mfrow=c(4,4), mar=c(2,2,2,2))


##--------------------##
# Simulation description
##--------------------##
 

# Nodes number [30;50]

range <- 30:50
n <- sample(range, 1)
n

# Network

net_simD <- network(n, directed = T)

# Density [0.1, 0.7]

dens <- runif(1,0.1,0.7)
dens

coefs <- c(qlogis(dens), 0)

# ERGM model

form1 <- net_simD ~ edges + mutual 

sim1 <- simulate(form1, coef = coefs)

gplot(sim1, vertex.cex = 1.5,
     main=paste("Density = ", round(dens, digits = 2) , 
                "; n = ", round(n, digits = 1))) 

#2

range <- 30:50
n <- sample(range, 1)
n

net_simD <- network(n, directed = T)

dens <- runif(1,0.1,0.7)
dens

coefs <- c(qlogis(dens), 0)

form1 <- net_simD ~ edges + mutual 

sim1 <- simulate(form1, coef = coefs)

gplot(sim1, vertex.cex = 1.5,
      main=paste("Density = ", round(dens, digits = 2) , 
                 "; n = ", round(n, digits = 1))) 

