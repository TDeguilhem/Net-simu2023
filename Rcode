      main=paste("Density = ", round(dens, digits = 2) , 
                 "; n = ", round(n, digits = 1))) 

#6

range <- 30:50
n <- sample(range, 1)
n

net_simD <- network(n, directed = T)

dens <- runif(1,0.1,0.7)
dens

coefs <- c(qlogis(dens), 0)

form1 <- net_simD ~ edges + mutual 

sim1 <- simulate(form1, coef = coefs)

gplot(sim1, vertex.cex = 1.5,
      main=paste("Density = ", round(dens, digits = 2) , 
                 "; n = ", round(n, digits = 1))) 

